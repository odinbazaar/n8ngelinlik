{
    "rewrites": [
        {
            "source": "/",
            "destination": "/bridal-studio-3section.html"
        },
        {
            "source": "/bridal",
            "destination": "/bridal-studio-3section.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ]
}